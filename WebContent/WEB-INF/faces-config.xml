<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<managed-bean>
		<managed-bean-name>fileUploadBean</managed-bean-name>
		<managed-bean-class>controle.negocio.FileUploadBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>perfumariaBean</managed-bean-name>
		<managed-bean-class>controle.negocio.PerfumariaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>gerenciadorCriticas</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorCriticas</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>gerenciadorDescontos</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorDescontos</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>gerenciadorHistorico</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorHistorico</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>		
	<managed-bean>
		<managed-bean-name>gerenciadorUsuario</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorUsuario</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerenciadorResumo</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorResumo</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerenciadorResumoFinanceiro</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorResumoFinanceiro</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerenciadorExportacao</managed-bean-name>
		<managed-bean-class>controle.negocio.GerenciadorExportacao</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<navigation-rule>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-action>#{odontologicoBean.pesquisaUsuario}</from-action>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
  		<navigation-case>
   			<from-outcome>criticas</from-outcome>
   			<to-view-id>/criticas.jsp</to-view-id>
   			<redirect/>
  		</navigation-case> 	
  		<navigation-case>
   			<from-outcome>testecriticas</from-outcome>
   			<to-view-id>/testecriticas.jsp</to-view-id>
   			<redirect/>
  		</navigation-case> 
		<navigation-case>
   			<from-outcome>upload</from-outcome>
   			<to-view-id>/fileUpload.jsp</to-view-id>
   			<redirect/>
  		</navigation-case>
		<navigation-case>
   			<from-outcome>index</from-outcome>
   			<to-view-id>/index.jsp</to-view-id>
   			<redirect/>
  		</navigation-case> 
		<navigation-case>
   			<from-outcome>login</from-outcome>
   			<to-view-id>/login.jsp</to-view-id>
   			<redirect/>
  		</navigation-case>   		
  		<navigation-case>
   			<from-outcome>descontos</from-outcome>
   			<to-view-id>/descontos.jsp</to-view-id>
   			<redirect/>
  		</navigation-case>  	
  		<navigation-case>
   			<from-outcome>extratoCriticas</from-outcome>
   			<to-view-id>/extratoCriticas.jsp</to-view-id>
   			<redirect/>
  		</navigation-case>  
  		<navigation-case>
   			<from-outcome>usuario</from-outcome>
   			<to-view-id>/usuario.jsp</to-view-id>
   			<redirect/>
  		</navigation-case> 	
  		<navigation-case>
   			<from-outcome>resumoCriticas</from-outcome>
   			<to-view-id>/resumoCriticas.jsp</to-view-id>
   			<redirect/>
  		</navigation-case> 
  		<navigation-case>
   			<from-outcome>resumoFinanceiro</from-outcome>
   			<to-view-id>/resumoFinanceiro.jsp</to-view-id>
   			<redirect/>
  		</navigation-case>
  		<navigation-case>
   			<from-outcome>mudancaSenha</from-outcome>
   			<to-view-id>/mudancaSenha.jsp</to-view-id>
   			<redirect/>
  		</navigation-case>  		
  		<navigation-case>
   			<from-outcome>exportaDescontos</from-outcome>
   			<to-view-id>/exportaDescontos.jsp</to-view-id>
   			<redirect/>
  		</navigation-case> 	  		
	</navigation-rule>

</faces-config>
